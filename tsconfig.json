{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": ["es2021", "dom"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"alwaysStrict": true,
		"noImplicitReturns": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*.ts"],
	"outDir": "./dist"
}
